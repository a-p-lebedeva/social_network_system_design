// Tarantool
//
// Replication:
// - master-slave (async)
// - replication factor 2
//
// Sharding:
// - key based by user_id

Table subscribed_posts {
  user_id UUID [pk, not null]
  post_list jsonb [not null]
 }

 Table celebrities_post {
  celebrity_id UUID [pk, not null]
  post_list jsonb [not null]
 }


//+ множество celebrity_id

